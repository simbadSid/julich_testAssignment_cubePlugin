uninstall-local:
	rm -rf $(DESTDIR)$(includedir)/cubelib
	rm -rf $(DESTDIR)$(datadir)/CubeLib
	rm -rf $(DESTDIR)$(datadir)/cubelib
	rm -rf $(DESTDIR)$(datadir)/icons/cubelib
	rm -rf $(DESTDIR)$(datadir)/cubelib
	rm -rf $(DESTDIR)$(datadir)/doc/cubelib
	rm -rf $(DESTDIR)$(datadir)/modulefiles/CubeLib

install-data-local:
	@echo "Install version information "
	$(MKDIR_P) $(DESTDIR)$(datadir)/cubelib$(AM_TARGET)
	@$(INSTALL_DATA) src/cubelib-version.h  $(DESTDIR)$(includedir)/cubelib/cubelib-version.h
	@$(INSTALL_DATA) cubelib.summary $(DESTDIR)$(datadir)/cubelib$(AM_TARGET)/cubelib.summary
	@echo "=============================================================="
	$(MKDIR_P) $(DESTDIR)$(datadir)/doc/cubelib
	$(MKDIR_P) $(DESTDIR)$(datadir)/icons/cubelib
	@$(INSTALL_DATA) $(srcdir)/share/icons/Cube.* $(DESTDIR)$(datadir)/icons/cubelib
	@$(INSTALL_DATA) $(srcdir)/share/icons/cubelogo.* $(DESTDIR)$(datadir)/icons/cubelib
	@$(INSTALL_DATA) $(srcdir)/share/icons/cubeicon.* $(DESTDIR)$(datadir)/icons/cubelib
	$(MKDIR_P) $(DESTDIR)$(datadir)/doc
	@$(INSTALL_DATA) $(srcdir)/doc/Cube*.pdf $(DESTDIR)$(datadir)/doc/cubelib
	@echo "=============================================================="

